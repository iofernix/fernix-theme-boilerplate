{
    "name": "fernix/wordpress-theme",
    "description": "",
    "version": "1.0.0",
    "type": "project",
    "keywords": [
        "wordpress",
        "theme"
    ],
    "homepage": "https://github.com/iofernix/wordpress-theme",
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Vargas",
            "email": "fernando.vargas@fernix.io"
        }
    ],
    "require-dev": {
        "fernix/wordpress-cli-composer": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Theme_Namespace\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "vendor-dir": "src/vendor"
    },
    "scripts": {
        "post-create-project-cmd": [
            "Fernix\\WordPress\\Theme\\Bootstrap::postCreateProject",
            "composer dump-autoload"
        ],
        "wp-install": [
            "Fernix\\WordPress\\WP::install"
        ]
    }
}
