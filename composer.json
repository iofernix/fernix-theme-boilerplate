{
    "name": "fernix/fernix-theme",
    "description": "",
    "version": "1.0.0",
    "type": "project",
    "keywords": [
        "wordpress",
        "theme"
    ],
    "homepage": "https://github.com/iofernix/fernix-theme-boilerplate",
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Vargas",
            "email": "fernando.vargas@fernix.io"
        }
    ],
    "require-dev": {
        "fernix/fernix-theme-bootstrap": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Theme_Namespace\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "vendor-dir": "src/vendor"
    },
    "scripts": {
        "post-create-project-cmd": [
            "Support\\Composer::postCreateProject",
            "composer dump-autoload"
        ],
        "wp-install": [
            "Support\\Composer::wpInstall"
        ]
    }
}
